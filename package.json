  "devDependencies": {
    "@jamphlet/eslint-config": "workspace:*",
    "@jamphlet/typescript-config": "workspace:*",
    "@types/node": "^20.10.4",
    "dotenv-cli": "^7.3.0",
    "prettier": "^3.1.0",
    "turbo": "latest",
    "typescript": "^5.2.2"
  },
  "packageManager": "pnpm@8.9.0",
  "engines": {
    "node": ">=18"
  }
}
